container_commands:
  00_collectstatic:
    command: |
      . /var/app/venv/*/bin/activate
      python manage.py collectstatic --noinput
    leader_only: true

  01_migrate:
    command: |
      . /var/app/venv/*/bin/activate
      python manage.py migrate --noinput
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: gkart.settings